		<ActiveObjectClass>
			<Id>1741955927177</Id>
			<Name><![CDATA[REceiver_BioNanomachine]]></Name>
			<StartupCode><![CDATA[// Αρχικοποίηση πινάκων ανάλογα με τα sectors
sectorAbsorptions_Total  = new double[numSectors];
sectorAbsorptions_Type0  = new double[numSectors];
sectorAbsorptions_Type1  = new double[numSectors];

sectorArrivalTimeSum     = new double[numSectors];
sectorArrivalCount       = new int[numSectors];
cumSectorCount_Type0 = new int[numSectors];
countType0_dt = new int[numSectors];
// --- absorption geometry init ---
absorbR = inRangeRadiusPx;   // το ίδιο R που είχες πριν (π.χ. 40 px)

// Square with SAME AREA as circle of radius R:
// Area(circle)=πR^2, Area(square)=side^2 => side=R*sqrt(π)
squareHalfSide = 0.5 * absorbR * Math.sqrt(Math.PI);

// Candidate radius that surely contains the whole square (to get candidates fast):
// half diagonal = halfSide*sqrt(2)
squareSearchR = squareHalfSide * Math.sqrt(2.0);
// =========================================
// Equilateral Triangle with SAME AREA as circle of radius absorbR
// Area(circle) = π R^2
// Area(triangle) = (sqrt(3)/4) * s^2
// => s = sqrt( 4A / sqrt(3) )
// =========================================
double A = Math.PI * absorbR * absorbR;

triSide   = Math.sqrt( (4.0 * A) / Math.sqrt(3.0) );
triHeight = (Math.sqrt(3.0) / 2.0) * triSide;

// Candidate radius for triangle (distance from centroid to vertex)
// For equilateral triangle: Rcircum = s / sqrt(3)
triSearchR = triSide / Math.sqrt(3.0);
traceln("[TRI] absorbR=" + absorbR
  + " A=" + A
  + " triSide=" + triSide
  + " triHeight=" + triHeight
  + " triSearchR=" + triSearchR);]]></StartupCode>
