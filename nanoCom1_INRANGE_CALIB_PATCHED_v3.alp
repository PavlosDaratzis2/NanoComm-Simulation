							<Code><![CDATA[1]]></Code>
							<Unit Class="RateUnits"><![CDATA[PER_SECOND]]></Unit>
						</Rate>
						<Id>1763245238590</Id>
						<OccurrenceAtTime>true</OccurrenceAtTime>
						<OccurrenceDate>1763280000000</OccurrenceDate>
						<OccurrenceTime Class="CodeUnitValue">
							<Code><![CDATA[0]]></Code>
							<Unit Class="TimeUnits"><![CDATA[SECOND]]></Unit>
						</OccurrenceTime>
						<RecurrenceCode Class="CodeUnitValue">
							<Code><![CDATA[msdSampleDt]]></Code>
							<Unit Class="TimeUnits"><![CDATA[SECOND]]></Unit>
						</RecurrenceCode>
						<Condition><![CDATA[false]]></Condition>
					</Properties>
					<Action><![CDATA[// === ΥΠΟΛΟΓΙΣΜΟΣ MSD(t) ΚΑΙ D(t) ΣΤΟ MAIN ===

// sum r^2 και πλήθος μορίων
double sumR2 = 0.0;
int count = 0;

// Διέτρεξε ΟΛΑ τα μόρια του πληθυσμού
for (Information_Molecules m : information_Moleculeses) {

    // Αν έχει σημαδευτεί για αφαίρεση, αγνόησέ το
    if (m.remove) 
        continue;

    // Διαφορά θέσης σε pixels από το origin (transmitter)
    double dx_px = m.getX() - m.originX;
    double dy_px = m.getY() - m.originY;

    // Μετατροπή σε μέτρα
    double dx_m = dx_px / pxPerMeter;
    double dy_m = dy_px / pxPerMeter;

    // r^2 = x^2 + y^2
    double r2 = dx_m * dx_m + dy_m * dy_m;

    sumR2 += r2;
    count++;
}

// Αν δεν υπάρχουν ενεργά μόρια, μην κάνεις τίποτα
if (count == 0) 
    return;

// MSD(t) = <r^2(t)>
double MSD = sumR2 / count;

// Τρέχων χρόνος προσομοίωσης
double t = time();
if (t <= 0)
    return;

// === ΔΙΑΧΥΣΗ ΣΕ 2D ΜΟΝΤΕΛΟ ΚΙΝΗΣΗΣ ===
// Για 2D:  MSD = 4 D t  =>  D = MSD / (4 t)
// (Αν θεωρούσαμε 3D: MSD = 6 D t, αλλά εδώ κινείσαι σε x,y)
double D = MSD / (4.0 * t);
// Προαιρετικό debug στην κονσόλα
targetD = D;
// Προαιρετικό debug στην κονσόλα
//traceln(String.format("t=%.2f  MSD=%.5f  D=%.5f", t, MSD, D));]]></Action>
