				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1747667239274</Id>
					<Name><![CDATA[checkCollisions]]></Name>
					<X>420</X><Y>300</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[/**
 * Συνάρτηση ανίχνευσης και χειρισμού ελαστικών συγκρούσεων μεταξύ μορίων.
 * Περιλαμβάνει:
 * 1. Εντοπισμό κοντινών μορίων μέσω spatial grid για βελτιωμένη απόδοση.
 * 2. Υπολογισμό αποστάσεων και γωνίας μεταξύ μορίων για να διαπιστωθεί εάν υπάρχει σύγκρουση.
 * 3. Ελαστική αλλαγή πορείας ±Φ/2 με βάση τη διχοτόμο, ή τυχαία ώθηση για αποφυγή ping-pong.
 * 4. Ενημέρωση headings και μετατόπιση μορίων.
 */

if (main.collisionsEnabled) { // ✅ Έλεγχος εάν το σύστημα επιτρέπει συγκρούσεις

    // === 1. Λήψη κοντινών μορίων από το spatial grid ===
    ArrayList<Information_Molecules> nearby = new ArrayList<>(); // Λίστα για τα κοντινά μόρια

    // Υπολογισμός του κελιού (στήλης και γραμμής) στο οποίο βρίσκεται το μόριο
    int myCol = (int) ((getX() - main.minX) / main.gridSize);
    int myRow = (int) ((getY() - main.minY) / main.gridSize);

    // Έλεγχος σε γειτονικά κελιά (3x3 περιοχή γύρω από το μόριο)
    for (int dx = -1; dx <= 1; dx++) {
        for (int dy = -1; dy <= 1; dy++) {
            int col = myCol + dx; // Στήλη προς έλεγχο
            int row = myRow + dy; // Γραμμή προς έλεγχο

            // Έλεγχος ότι το κελί είναι εντός των ορίων του grid
            if (col >= 0 && col < main.cols && row >= 0 && row < main.rows) {
                ArrayList<Information_Molecules> cell = main.spatialGrid[col][row]; // Παίρνουμε τα μόρια από αυτό το κελί
                if (cell != null)
                    nearby.addAll(cell); // Προσθήκη όλων των μορίων από αυτό το κελί στη λίστα "nearby"
            }
        }
    }

    // === 2. Έλεγχος για συγκρούσεις με τα κοντινά μόρια ===
    for (Information_Molecules other : nearby) {
        if (other == this) continue; // Αγνόηση του εαυτού

        // Υπολογισμός διαφοράς στις συντεταγμένες μεταξύ των δύο μορίων
        double dx = this.getX() - other.getX();
        double dy = this.getY() - other.getY();
        double distSquared = dx * dx + dy * dy; // Τετράγωνο της απόστασης (για αποφυγή sqrt)

        // Αν η απόσταση είναι μικρότερη από 4.5, θεωρούμε ότι υπάρχει σύγκρουση
        if (distSquared < 4.5 * 4.5) {
            main.totalCollisions++; // ➕ Ενημέρωση συνολικού αριθμού συγκρούσεων

            double dist = Math.sqrt(distSquared); // Απόσταση
            if (dist == 0) dist = 0.01; // Προστασία από διαίρεση με το μηδέν

            // Μονάδες διεύθυνσης (διανύσματα normal σύγκρουσης)
            double nx = dx / dist;
            double ny = dy / dist;

            // Διανύσματα ταχύτητας βάσει Heading
            double vAx = Math.cos(toRadians(this.Heading));
            double vAy = Math.sin(toRadians(this.Heading));
            double vBx = Math.cos(toRadians(other.Heading));
            double vBy = Math.sin(toRadians(other.Heading));

            // Εσωτερικό γινόμενο για υπολογισμό γωνίας Φ
            double dotProduct = vAx * vBx + vAy * vBy;
            double magA = Math.sqrt(vAx * vAx + vAy * vAy); // Μέτρο ταχύτητας this
            double magB = Math.sqrt(vBx * vBx + vBy * vBy); // Μέτρο ταχύτητας other
            if (magA == 0 || magB == 0) continue; // Αγνόηση σταματημένων μορίων

            double anglePhiRad = Math.acos(dotProduct / (magA * magB)); // Γωνία Φ σε ακτίνια
            double anglePhiDeg = toDegrees(anglePhiRad); // Φ σε μοίρες

            // === 3. Αν Φ < 30°, απόφυγε το ping-pong με τυχαία ώθηση ===
            if (anglePhiDeg < 30) {
                double randAngle1 = uniform(0, 360); // Τυχαία γωνία για το πρώτο μόριο
                double randAngle2 = randAngle1 + 180; // Αντίθετη κατεύθυνση για το δεύτερο
                double randPush = 1.0; // Απόσταση ώθησης

                // Εφαρμογή μετατόπισης
                this.setXY(
                    this.getX() + randPush * Math.cos(toRadians(randAngle1)),
                    this.getY() + randPush * Math.sin(toRadians(randAngle1))
                );
                other.setXY(
                    other.getX() + randPush * Math.cos(toRadians(randAngle2)),
                    other.getY() + randPush * Math.sin(toRadians(randAngle2))
                );

                // Νέες τυχαίες πορείες για αποφυγή συνεχόμενων συγκρούσεων
                this.Heading = uniform(0, 360);
                other.Heading = uniform(0, 360);
                continue; // Προχώρα στο επόμενο ζεύγος
            }

            // === 4. Υπολογισμός διχοτόμου της γωνίας Φ ===
            double mx = vAx + vBx; // Άθροισμα x-συνιστωσών
            double my = vAy + vBy; // Άθροισμα y-συνιστωσών
            double magM = Math.sqrt(mx * mx + my * my);
            if (magM == 0) { // Αν είναι τελείως αντίθετα
                mx = -ny; my = nx; // Τεχνητή κάθετη διχοτόμος
                magM = Math.sqrt(mx * mx + my * my);
            }
            double bisectorX = mx / magM;
            double bisectorY = my / magM;

            // Περιστροφή κατά ±Φ/2 γύρω από τη διχοτόμο
            double angleOffset = anglePhiRad / 2;

            // Νέο διάνυσμα για το πρώτο μόριο (περιστροφή -Φ/2)
            double newAx = Math.cos(angleOffset) * bisectorX - Math.sin(angleOffset) * bisectorY;
            double newAy = Math.sin(angleOffset) * bisectorX + Math.cos(angleOffset) * bisectorY;

            // Νέο διάνυσμα για το δεύτερο μόριο (περιστροφή +Φ/2)
            double newBx = Math.cos(-angleOffset) * bisectorX - Math.sin(-angleOffset) * bisectorY;
            double newBy = Math.sin(-angleOffset) * bisectorX + Math.cos(-angleOffset) * bisectorY;

            // === 5. Ενημέρωση των headings των μορίων ===
            this.Heading = toDegrees(Math.atan2(newAy, newAx));
            other.Heading = toDegrees(Math.atan2(newBy, newBx));

            // === 6. Ήπια ώθηση για απομάκρυνση ===
            double pushDist = 1.4; // Απόσταση ώθησης
            this.setXY(this.getX() + newAx * pushDist, this.getY() + newAy * pushDist);
            other.setXY(other.getX() + newBx * pushDist, other.getY() + newBy * pushDist);

            // === 7. Αν είναι υπερβολικά κοντά, τυχαία μικρή μετατόπιση επιπλέον ===
            if (dist < 1.2) {
                double randAngle1 = uniform(0, 360);
                double randAngle2 = randAngle1 + 180;

                this.setXY(
                    this.getX() + 0.5 * Math.cos(toRadians(randAngle1)),
                    this.getY() + 0.5 * Math.sin(toRadians(randAngle1))
                );
                other.setXY(
                    other.getX() + 0.5 * Math.cos(toRadians(randAngle2)),
                    other.getY() + 0.5 * Math.sin(toRadians(randAngle2))
                );
            }
        }
    }
}]]></Body>
