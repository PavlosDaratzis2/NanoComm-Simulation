				<Event>
					<Id>1765998429179</Id>
					<Name><![CDATA[absorbFeedbackMolecules]]></Name>
					<X>-240</X><Y>-130</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Properties TriggerType="timeout" Mode="cyclic">
						<Timeout Class="CodeUnitValue">
							<Code><![CDATA[1]]></Code>
							<Unit Class="TimeUnits"><![CDATA[SECOND]]></Unit>
						</Timeout>
						<Rate Class="CodeUnitValue">
							<Code><![CDATA[1]]></Code>
							<Unit Class="RateUnits"><![CDATA[PER_SECOND]]></Unit>
						</Rate>
						<Id>1765998429177</Id>
						<OccurrenceAtTime>true</OccurrenceAtTime>
						<OccurrenceDate>1766044800000</OccurrenceDate>
						<OccurrenceTime Class="CodeUnitValue">
							<Code><![CDATA[0.1]]></Code>
							<Unit Class="TimeUnits"><![CDATA[SECOND]]></Unit>
						</OccurrenceTime>
						<RecurrenceCode Class="CodeUnitValue">
							<Code><![CDATA[0.1]]></Code>
							<Unit Class="TimeUnits"><![CDATA[SECOND]]></Unit>
						</RecurrenceCode>
						<Condition><![CDATA[false]]></Condition>
					</Properties>
					<Action><![CDATA[// (προαιρετικό debug για να δεις ότι τρέχει)
// traceln("TX absorb event fired t=" + time());

List<Information_Molecules> nearby = agentsInRange(main.information_Moleculeses, txAbsorbRadius);

int absorbedNow = 0;

for (Information_Molecules m : nearby) {

    // ΜΟΝΟ feedback
    if (m.type == null || !m.type.trim().equals("F0")) continue;

    if (!m.absorbed && !m.remove) {

        // απορρόφηση
        m.stop();
        m.set_absorbed(true);
		m.oval.setFillColor(Color.GREEN);
        // ΚΑΘΑΡΙΣΜΑ (αν το χρησιμοποιείς στο project σου)
        m.remove = true;

        // γωνία άφιξης γύρω από Tx (fixed global axis)
        double angle   = atan2(-(m.getY() - getY()), m.getX() - getX());
        double degrees = toDegrees(angle);
        if (degrees < 0) degrees += 360;

        double sectorWidth = 360.0 / txNumSectors;   // 45 deg όταν txNumSectors=8
        int sector = (int)(degrees / sectorWidth);

        if (sector < 0) sector = 0;
        if (sector >= txNumSectors) sector = txNumSectors - 1;
        // 🎨 ΧΡΩΜΑ ΑΝΑ SECTOR
		// 🎨 ΕΝΤΟΝΑ ΧΡΩΜΑΤΑ ΑΝΑ SECTOR (ΧΩΡΙΣ ΚΟΚΚΙΝΟ & ΠΡΑΣΙΝΟ)
		switch (sector) {
    		case 0: m.oval.setFillColor(Color.BLUE); break;
    		case 1: m.oval.setFillColor(Color.CYAN); break;
    		case 2: m.oval.setFillColor(Color.MAGENTA); break;		
    		case 3: m.oval.setFillColor(Color.ORANGE); break;
    		case 4: m.oval.setFillColor(Color.YELLOW); break;
    		case 5: m.oval.setFillColor(Color.PINK); break;
    		case 6: m.oval.setFillColor(new Color(0, 0, 139)); break;   // Dark Blue
    		case 7: m.oval.setFillColor(new Color(128, 0, 128)); break; // Purple
			}

        // counters
        txFeedbackCountBySector[sector] += 1;

        // arrival time
        double tArr = time() - m.birthTime;
        if (tArr > 0) {
            txFeedbackArrivalTimeSum[sector] += tArr;
            txFeedbackArrivalCount[sector]   += 1;
            traceln("TX ABSORB t=" + time()
        + " sector=" + sector
        + " totalInSector=" + txFeedbackCountBySector[sector]);
        }

        absorbedNow++;
    }
}

if (absorbedNow > 0) {
   traceln("TX absorbed feedback: +" + absorbedNow + " at t=" + time());
    //traceln("TX sector=" + sector + " totalInSector=" + txFeedbackCountBySector[sector]);
}]]></Action>
