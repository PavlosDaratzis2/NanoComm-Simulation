					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[
double dt = main.modelDt;
double step = speed * dt * main.pxPerMeter;

// μικρή τυχαία διακύμανση στη διεύθυνση (0 = καθόλου, 180 = πλήρως τυχαία)
Heading += uniform(-15, 15);   // π.χ. ±15° τυχαία απόκλιση κάθε tick

// ενημέρωση θέσης
double newX = getX() + step * cos(toRadians(Heading));
double newY = getY() + step * sin(toRadians(Heading));

moveTo(newX, newY);


]]></Body>
