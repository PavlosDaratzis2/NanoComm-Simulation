				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1765988727217</Id>
					<Name><![CDATA[sendCumulativeFeedback0]]></Name>
					<X>-490</X><Y>-170</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[if (!main.feedbackEnabled) {
    traceln("sendCumulativeFeedback0: blocked (feedbackEnabled=false)");
    return;
}


// πόσα feedback μόρια θα στείλουμε σε αυτό το feedback pulse
int totalFeedback = main.bacteriaPerPulse;
if (totalFeedback <= 0) return;

int S = numSectors;
if (S <= 0) return;

if (cumSectorCount_Type0 == null || cumSectorCount_Type0.length != S) {
    traceln("sendCumulativeFeedback0 ERROR: cumSectorCount_Type0 not initialized / wrong length");
    return;
}

// 1) sum απορροφήσεων Type0 ανά sector
int sumCum = 0;
int activeSectors = 0;
for (int s = 0; s < S; s++) {
    sumCum += cumSectorCount_Type0[s];
    if (cumSectorCount_Type0[s] > 0) activeSectors++;
}

if (sumCum <= 0) {
    traceln("sendCumulativeFeedback0: sumCum=0 -> no feedback sent");
    return;
}

// 2) αναλογική κατανομή totalFeedback στα sectors
int[] nPerSector = new int[S];
double[] frac = new double[S];

int assigned = 0;

for (int s = 0; s < S; s++) {

    if (cumSectorCount_Type0[s] <= 0) {
        // ΚΟΨΙΜΟ: αν δεν απορρόφησε τίποτα, ΔΕΝ στέλνει τίποτα
        nPerSector[s] = 0;
        frac[s] = -1e9;   // ώστε να μην επιλεγεί ποτέ στα "remaining"
        continue;
    }

    double exact = (cumSectorCount_Type0[s] * 1.0 * totalFeedback) / sumCum;
    int n = (int) Math.floor(exact);

    nPerSector[s] = n;
    frac[s] = exact - n;
    assigned += n;
}

// 3) μοίρασμα υπολοίπων ώστε να γίνει ακριβώς totalFeedback
int remaining = totalFeedback - assigned;

// IMPORTANT: το remaining ΠΡΕΠΕΙ να μοιραστεί ΜΟΝΟ σε cum>0 sectors
while (remaining > 0) {

    int best = -1;

    for (int s = 0; s < S; s++) {
        if (cumSectorCount_Type0[s] <= 0) continue; // <-- ΚΡΙΣΙΜΟ
        if (best == -1 || frac[s] > frac[best]) best = s;
    }

    if (best == -1) break; // safety (δεν θα συμβεί αν sumCum>0)

    nPerSector[best] += 1;

    // ΜΗΝ κάνεις frac=-1 (αυτό δημιουργεί λάθος κατανομή)
    // Απλά το μειώνουμε κατά 1 ώστε να μπορεί να ξαναεπιλεγεί αν χρειαστεί.
    frac[best] -= 1.0;

    remaining--;
}

// ---- DEBUG: δες καθαρά ποιος στέλνει και γιατί ----
traceln("---- FEEDBACK SPAWN SUMMARY t=" + time()
        + " | totalFeedback=" + totalFeedback
        + " | S=" + S
        + " | sumCum=" + sumCum
        + " | activeSectors=" + activeSectors);

for (int s = 0; s < S; s++) {
    if (nPerSector[s] > 0) {
        traceln("sector " + s + ": send=" + nPerSector[s] + " (cum=" + cumSectorCount_Type0[s] + ")");
    }
}

// 4) spawn από το αντίστοιχο sector
double sectorWidthDeg = 360.0 / S;

// radius του circle του receiver στο animation (π.χ. 40)
double receiverBodyRadiusPx = 40;
double rReleasePx = receiverBodyRadiusPx + 2;

int created = 0;

for (int s = 0; s < S; s++) {

    int n = nPerSector[s];
    if (n <= 0) continue;

    double startDeg = s * sectorWidthDeg;
    double endDeg   = (s + 1) * sectorWidthDeg;

    for (int i = 0; i < n; i++) {

        double deg = uniform(startDeg, endDeg);
        double theta = Math.toRadians(deg);

        double xSpawn = getX() + rReleasePx * Math.cos(theta);
        double ySpawn = getY() - rReleasePx * Math.sin(theta);

        // ΧΡΗΣΙΜΟΠΟΙΕΙΣ ΤΟ ΣΩΣΤΟ creation που έχεις ήδη στον Tx:
        Information_Molecules m = main.add_information_Moleculeses();
        m.jumpTo(xSpawn, ySpawn);

        m.type = "F0";
        m.birthTime = time();
        m.set_absorbed(false);
        m.remove = false;
        m.oval.setFillColor( Color.BLUE );

        created++;
    }
}

traceln("sendCumulativeFeedback0 DONE created=" + created
        + " target=" + totalFeedback
        + " numSectors=" + S
        + " sumCum=" + sumCum
        + " t=" + time());]]></Body>
