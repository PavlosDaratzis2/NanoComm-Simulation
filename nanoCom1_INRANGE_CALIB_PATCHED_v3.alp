							<Code><![CDATA[0.01]]></Code>
							<Unit Class="TimeUnits"><![CDATA[SECOND]]></Unit>
						</RecurrenceCode>
						<Condition><![CDATA[false]]></Condition>
					</Properties>
					<Action><![CDATA[// ==========================================================
//  ABSORB MOLECULES (Receiver)
//  - Supports absorption geometry: CIRCLE / SQUARE / TRIANGLE
//    via: main.absorptionShapeId
//      0 = circle, 1 = square, 2 = equilateral triangle
//  - Uses helper functions in Receiver:
//      double getCandidateRadius()
//      boolean isInsideAbsorption(double x, double y)
//
//  Keeps your existing logic:
//  - ignores F0 (feedback)
//  - capacity limit receptorSize counts ONLY DATA
//  - sectors computed by angle (same as before)
//  - updates counters, plots, message detection
//  - Δt export counters: main.countType0_dt[sector]
//  - total sums: main.sumType0_all[sector]
// ==========================================================


// === 1) SELECT CANDIDATES NEAR RECEIVER (depends on geometry) ===
double candR = getCandidateRadius();
List<Information_Molecules> candidates = agentsInRange(main.information_Moleculeses, candR);

// Filter candidates strictly inside the current absorption shape
List<Information_Molecules> nearbyMolecules = new ArrayList<>();
for (Information_Molecules m : candidates) {
    if (isInsideAbsorption(m.getX(), m.getY())) {
        nearbyMolecules.add(m);
    }
}


// === 1b) Measure ONLY DATA (not F0) for receptorSize capacity ===
int dataNearbyCount = 0;
for (Information_Molecules x : nearbyMolecules) {
    if (x.type == null || !x.type.trim().equals("F0")) {
        dataNearbyCount++;
    }
}


// === 2) ABSORB MOLECULES ===
List<Information_Molecules> absorbedMoleculesA = new ArrayList<>();

for (Information_Molecules m : nearbyMolecules) {

    // 2.0 Ignore feedback molecules (Rx→Tx)
    if (m.type != null && m.type.trim().equals("F0")) {
        continue;
    }

    // 2.1 Capacity: based ONLY on DATA molecules
    if (!m.absorbed && !m.remove && dataNearbyCount <= main.receptorSize) {

        // 2.2 Absorb molecule
        m.stop();
        m.set_absorbed(true);

        // 2.3 Compute angle & sector (AnyLogic inverted Y)
        double angle   = atan2(-(m.getY() - getY()), m.getX() - getX());
        double degrees = toDegrees(angle);
        if (degrees < 0) degrees += 360;

        double sectorWidth = 360.0 / numSectors;
        int sector = (int)(degrees / sectorWidth); // 0..numSectors-1

        if (sector < 0) sector = 0;
        if (sector >= numSectors) sector = numSectors - 1;

        // 2.4 Arrival time
        double tArr = time() - m.birthTime;
        if (tArr > 0) {
            sectorArrivalTimeSum[sector] += tArr;
            sectorArrivalCount[sector]   += 1;
        }

        // 2.5 Total absorptions per sector (all DATA)
        sectorAbsorptions_Total[sector] += 1.0;

        // 2.6 If molecule is Type0 ("0")
        if (m.type != null && m.type.trim().equals("0")) {

            // cumulative sector profile (used for feedback distribution)
            cumSectorCount_Type0[sector] += 1;

            // threshold detection counter (in-range logic)
            main.absorbedType0SinceLastReport++;

            // Δt export counter (arrivals within current Δt)
            main.countType0_dt[sector] += 1;

            // global total sum for whole simulation (footer TOTAL)
            main.sumType0_all[sector] += 1;
        }

        // 2.7 Global absorbed counter
        main.totalAbsorbedAll++;

        // 2.8 Update charts (as you already have)
        main.updateBarAbsorptionsTotal();
        main.updateBarAbsorptionsType0();
        main.updateBarAbsorptionsType1(); // keep if still exists
        main.updateBarArrivalTime();
    }
}


// === 3) VISUALIZE absorbed molecules ===
for (Information_Molecules m : nearbyMolecules) {
    if (m.absorbed) {
        absorbedMoleculesA.add(m);
        m.oval.setFillColor(Color.RED);
    }
}


// === 4) TOTAL MESSAGE detection (A) ===
if (!main.passedThresholdA && absorbedMoleculesA.size() >= main.individualThreshold) {
    main.passedThresholdA = true;
}
if (main.passedThresholdA && absorbedMoleculesA.size() <= main.individualDownThreshold) {
    main.passeddownThresholdA = true;
}
if (main.passedThresholdA && main.passeddownThresholdA) {
    main.NUMBER_OF_MESSAGES_RECEIVED++;
    main.passedThresholdA = false;
    main.passeddownThresholdA = false;
}


// === 5) TYPE0 message detection (thresholds) ===
if (!main.passedThresholdType0 && main.absorbedType0SinceLastReport >= main.thresholdType0_Upper) {
    main.passedThresholdType0 = true;
}
if (main.passedThresholdType0 && main.absorbedType0SinceLastReport <= main.thresholdType0_Lower) {
    main.passedDownThresholdType0 = true;
}
if (main.passedThresholdType0 && main.passedDownThresholdType0) {

    // feedback send is NOT here (you do it from event)
    // sendCumulativeFeedback0();

    main.messagesReceivedType0++;
    main.datasetInRange_Type0.add(time(), main.messagesReceivedType0);

    main.passedThresholdType0 = false;
    main.passedDownThresholdType0 = false;
    main.absorbedType0SinceLastReport = 0;
}


// === 6) TYPE1 block (keep as-is if exists) ===
if (!main.passedThresholdType1 && main.absorbedType1SinceLastReport >= main.thresholdType1_Upper) {
    main.passedThresholdType1 = true;
}
if (main.passedThresholdType1 && main.absorbedType1SinceLastReport <= main.thresholdType1_Lower) {
    main.passedDownThresholdType1 = true;
}
if (main.passedThresholdType1 && main.passedDownThresholdType1) {

    main.messagesReceivedType1++;
    main.datasetInRange_Type1.add(time(), main.messagesReceivedType1);

    main.passedThresholdType1 = false;
    main.passedDownThresholdType1 = false;
    main.absorbedType1SinceLastReport = 0;
}


// === 7) Cleanup ===
absorbedMoleculesSinceLastReportA = absorbedMoleculesA.size();
if (absorbedMoleculesA.size() == 0) {
    main.passedThresholdA = false;
}


// === 8) Update arrival bar (already in your project) ===
main.updateBarArrivalTime();]]></Action>
