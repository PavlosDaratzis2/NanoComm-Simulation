						</Rate>
						<Id>1747753877012</Id>
						<OccurrenceAtTime>true</OccurrenceAtTime>
						<OccurrenceDate>1747814400000</OccurrenceDate>
						<OccurrenceTime Class="CodeUnitValue">
							<Code><![CDATA[0]]></Code>
							<Unit Class="TimeUnits"><![CDATA[SECOND]]></Unit>
						</OccurrenceTime>
						<RecurrenceCode Class="CodeUnitValue">
							<Code><![CDATA[0.01]]></Code>
							<Unit Class="TimeUnits"><![CDATA[SECOND]]></Unit>
						</RecurrenceCode>
						<Condition><![CDATA[false]]></Condition>
					</Properties>
					<Action><![CDATA[// =====================
// Signal = MOLECULES IN RANGE (Type0 DATA only)
// Calibration on first pulse -> URT/LRT for CURRENT bacteriaPerPulse
// Detection with URT/LRT -> feedback send
// =====================

// ---- 0) Track current run N dynamically ----
if (currentNtx0 <= 0) currentNtx0 = -1;

// ---- 1) Raw signal: molecules in range (integer) ----
int yRaw = rEceiver_BioNanomachine.reportMoleculesA(); // raw integer count
winAbs0 = (double) yRaw;                               // store raw as double

// ---- 1b) EWMA smoothing (gives fractional values like 11.66) ----
// IMPORTANT: initialize filter the first time, αλλιώς ξεκινάει από 0 και κάνει περίεργα
if (!yFilt0_init) {
    yFilt0 = winAbs0;
    yFilt0_init = true;
} else {
    // EWMA: yFilt = alpha*yRaw + (1-alpha)*yFilt
    yFilt0 = alpha0 * winAbs0 + (1.0 - alpha0) * yFilt0;
}

// Αυτό είναι ΤΟ ΣΗΜΑ που χρησιμοποιεί το detection
double ySignal = yFilt0;

// Expose to monitors/plots
// Αν absorbedType0InRange είναι double -> αυτό είναι ΟΚ:
absorbedType0InRange = ySignal;

// Αν absorbedType0InRange είναι int, τότε ΑΝΤΙ για την από πάνω γραμμή βάλε:
// absorbedType0InRange = (int) Math.round(ySignal);

absorbedType1InRange = 0;


// ---- 2) AUTO-CALIBRATION on the FIRST pulse ----
if (!thresholdsReady && !calibrating && winAbs0 > 0) {

    calibrating = true;
    calibrationStartTime = time();

    currentNtx0 = bacteriaPerPulse;

    double r_m = transmitter1_BioNanomachine.distanceTo(rEceiver_BioNanomachine) / pxPerMeter;
    double D_est = targetD;
    double td = (D_est > 0) ? (r_m * r_m) / (6.0 * D_est) : 0;

    if (td > 0) calibrationTmax = td;
    else        calibrationTmax = 80.0;

    datasetInRange_Type0.reset();

    passedUpConcType0 = false;
    passedDownConcType0 = false;

    traceln("CALIBRATION START t=" + time()
            + "  Ntx=" + currentNtx0
            + "  alpha0=" + alpha0
            + "  Tmax=" + calibrationTmax);
}

if (calibrating) {

    double tRel = time() - calibrationStartTime;

    // store the SAME signal we detect on (filtered)
    datasetInRange_Type0.add(tRel, ySignal);

    if (tRel >= calibrationTmax) {

        int M = datasetInRange_Type0.size();
        if (M < 5) {
            traceln("CALIBRATION: not enough samples (M=" + M + ")");
            calibrating = false;
        } else {

            double maxY = 0;
            for (int i = 0; i < M; i++) {
                double yy = datasetInRange_Type0.getY(i);
                if (yy > maxY) maxY = yy;
            }

            // URT = round(0.9 * maxY)
            int URT = (int) Math.round(0.9 * maxY);
            if (URT < 2) URT = 2;

            int LRT = (int) Math.round(URT / 3.0);
            if (LRT < 1) LRT = 1;
            if (LRT >= URT) LRT = URT - 1;

            thresholdType0_Upper = URT;
            thresholdType0_Lower = LRT;

            thresholdsReady = true;
            calibrating = false;

            traceln("CALIBRATION DONE t=" + time()
                    + "  URT=" + URT
                    + "  LRT=" + LRT
                    + "  maxY(filtered)=" + maxY);
        }
    }
}


// ---- 3) MESSAGE DETECTION (URT/LRT) ----
if (thresholdsReady) {

    double y = ySignal;

    double upper = (double) thresholdType0_Upper;
    double lower = (double) thresholdType0_Lower;

    if (!passedUpConcType0 && y >= upper) {
        passedUpConcType0 = true;
        traceln(">>> START detected at t=" + time()
                + " (yFilt=" + y + ", yRaw=" + yRaw + ")");
    }

    if (passedUpConcType0 && y <= lower) {
        traceln(">>> END-OF-MSG detected at t=" + time()
                + " (yFilt=" + y + ", yRaw=" + yRaw + ")");
        NUMBER_OF_MESSAGES_RECEIVED++;

        // feedback (αν το θες OFF-by-default, βάλε εδώ if(flag))
        rEceiver_BioNanomachine.sendCumulativeFeedback0();

        for (int s = 0; s < rEceiver_BioNanomachine.numSectors; s++) {
            rEceiver_BioNanomachine.cumSectorCount_Type0[s] = 0;
        }

        passedUpConcType0 = false;
        passedDownConcType0 = false;
    }
}]]></Action>
