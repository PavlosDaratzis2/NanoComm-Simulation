			<StartupCode><![CDATA[// ====== SETUP CSV EXPORT (wide format) ======

int S = rEceiver_BioNanomachine.numSectors;
if (S <= 0) S = 20;

// (1) Βεβαιώσου ότι τα arrays έχουν σωστό μήκος
if (countType0_dt == null || countType0_dt.length != S) {
    countType0_dt = new int[S];
}
if (sumType0_all == null || sumType0_all.length != S) {
    sumType0_all = new int[S];
}

// (2) Μηδενισμοί
for (int s = 0; s < S; s++) {
    countType0_dt[s] = 0;
    sumType0_all[s] = 0;
}

// (3) Start χρόνου παραθύρου
tWindowStart = time();

// (4) Άνοιγμα writer
try {
    csvOut = new java.io.PrintWriter(
        new java.io.BufferedWriter(
            new java.io.FileWriter(csvFile, false)   // overwrite
        )
    );

    // Header: tStart,tEnd,s0..s(S-1),total
    String header = "tStart,tEnd";
    for (int s = 0; s < S; s++) header += ",s" + s;
    header += ",total";

    csvOut.println(header);
    csvOut.flush();

} catch (Exception e) {
    traceln("CSV OPEN ERROR: " + e);
    csvOut = null;
}

traceln("CSV path = " + csvFile.getAbsolutePath());]]></StartupCode>
			<DestroyCode><![CDATA[if (csvOut == null) return;
if (sumType0_all == null) return;

int S = sumType0_all.length;

int grandTotal = 0;

// TOTAL στη στήλη tStart, κενό στη στήλη tEnd, μετά αμέσως sectors
String footer = "TOTAL,";   // ✅ ΜΟΝΟ 1 κενό κελί (tEnd)

for (int s = 0; s < S; s++) {
    footer += "," + sumType0_all[s];
    grandTotal += sumType0_all[s];
}

footer += "," + grandTotal; // τελευταία στήλη: overall total

csvOut.println(footer);
csvOut.flush();]]></DestroyCode>
